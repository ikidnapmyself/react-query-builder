{
  "name": "headless-react-query-builder",
  "version": "0.1.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./templates/tailwind": {
      "types": "./dist/templates/tailwind/index.d.ts",
      "import": "./dist/templates/tailwind.js",
      "require": "./dist/templates/tailwind.cjs"
    },
    "./templates/bootstrap": {
      "types": "./dist/templates/bootstrap/index.d.ts",
      "import": "./dist/templates/bootstrap.js",
      "require": "./dist/templates/bootstrap.cjs"
    },
    "./templates/unstyled": {
      "types": "./dist/templates/unstyled/index.d.ts",
      "import": "./dist/templates/unstyled.js",
      "require": "./dist/templates/unstyled.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsc -b && vite build",
    "lint": "eslint src/",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@eslint/js": "^9.39.2",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/addon-vitest": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "bootstrap": "^5.3.8",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.2.8",
    "jsdom": "^27.0.1",
    "playwright": "^1.58.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "storybook": "^10.2.8",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "react",
    "query-builder",
    "conditional-logic",
    "rule-engine",
    "alert-manager",
    "automation"
  ],
  "license": "MIT",
  "description": "A composable, headless React 19 query builder component for conditional logic UIs"
}
